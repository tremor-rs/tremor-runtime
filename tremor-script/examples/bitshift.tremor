emit [
  # right bit shift
  42 >> 0,
  42 >> 2,
  -42 >> 2,
  42 >> 63,

  # right bit shift (invalid)
  #42 >> 64,
  #42 >> -1,
  #42 >> 2.0,
  #42 >> "2",
  #42 >> true
  #42 >>> 2

  # left bit shift
  42 << 0,
  42 << 2,
  -42 << 2,
  42 << 63

  # left bit shift(invalid)
  #42 << 64
  #42 << -1
  #42 << 2.0
  #42 << "2"
  #42 << true
  #42 <<< 2
];
