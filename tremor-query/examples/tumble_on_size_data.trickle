# pairs
define tumbling window pairs
with
   size = 2,
script
   2
end;

define tumbling window triples
with
   size = 3,
end;

define tumbling window quads
with
   size = 4,
end;


select {
    "window": window,
    "count": stats::count(),
    "other": win::collect_flattened(event.value),
}
from in[pairs, triples, quads]
into out;
