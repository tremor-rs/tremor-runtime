
define flow main
flow
  use tremor::pipelines;
  use bench;
  create connector bench from bench::bench
  with
    file = "data.influx.xz",
    codec = "influx"
  end;
  create pipeline main from pipelines::passthrough;

  connect /connector/bench to /pipeline/main;
  connect /pipeline/main to /connector/bench;
end;
deploy flow main;