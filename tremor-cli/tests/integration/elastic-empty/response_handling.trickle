select {
  "action": $elastic.action,
  "success": $elastic.success,
  "payload": event.payload,
  "index": $elastic["_index"],
  "doc": $elastic["_type"]
}
from in where $elastic.success into out;

select {
  "action": $elastic.action,
  "success": $elastic.success,
  "payload": event.payload,
