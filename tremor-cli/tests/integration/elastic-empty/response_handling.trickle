select {
  "success": event.success,
  "payload": event.payload,
  "index": $elastic.index,
  "doc": $elastic.doc_type
}
from in where event.success into out;

select {
  "payload": event.payload,
  "success": event.success,
  "index": $elastic.index,
  "doc": $elastic.doc_type
}
from in where not event.success into err;