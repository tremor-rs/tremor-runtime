  insert "cc" => patch {} of insert "fleek" => "florp" end,
  update "bb" =>  3,
  upsert "aa" => 4,
  upsert "dd" => 5,
  move "dd" => "ff",
  copy "ff" => "dd",
  erase "aa",
  merge => { "fleek": "flork" },
  merge "cc" => {"flip": "flop"},
