define tumbling window by_two
with
  size = 2
end;

mod m with
    aggregate collect_joined of
        init ( ) =>
            "not an empty string"

        aggregate(st, data) =>
            st + data

        mergein(st, other) =>
            st + other

        emit(st) =>
            st
    end
end;

select m::collect_joined(event) from in[by_two] into out;
